@model ASOFT.ERP.ViewModel.POS.POSF0066ViewModel

<div id="payment_container">
    <div class="clearfix"></div>
    <div class="pos_header">

        <h3>@Html.ASOFTRaw(model => model.PaymentTitle)</h3>
