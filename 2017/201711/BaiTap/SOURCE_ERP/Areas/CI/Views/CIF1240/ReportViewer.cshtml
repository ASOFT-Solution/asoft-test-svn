
@using ASOFT.ERP.A00.Core;

@{
    Layout = null;
    if (ViewBag.ActionPrint == 1)
    {
        ViewBag.Action = Url.Action("ExportReport", "CIF1240", new
        {
            id = ViewBag.APK,
            viewer = ViewBag.Viewer
        });
    }
    if (ViewBag.ActionPrint == 2)
    {
        ViewBag.Action = Url.Action("ExportReport", "CIF1240", new
        {
            id = ViewBag.APK,
            viewer = ViewBag.Viewer,
            actionPrint = "2"
        });
    }
}  

@Html.Partial("~/Views/Shared/_ReportLayout.cshtml")