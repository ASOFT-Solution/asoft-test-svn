@using ASOFT.ERP.A00.Core.Control;
@using ASOFT.ERP.A00.Core;
@using ASOFT.ERP.A00.Entities.Infrastructure;
@using ASOFT.ERP.A00.Entities.Database;
@using ASOFT.ERP.A00.Entities.Common;
@using ASOFT.ERP.ViewModel;
@using System.Web.UI.WebControls;
@using ASOFT.ERP.ViewModel.CI;
@using Kendo.Mvc;
@model ASOFT.ERP.ViewModel.CI.CIF1012ViewModel
@{
    ViewBag.Title = Html.ASOFTPageTitle(m => m.Title, "CIF1012").ToHtmlString();
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section jsFeature
{
    <script src="@Url.Content("~/Areas/CI/Scripts/CIF1010.js?v=" + ASOFTEnvironment.Version)"></script>
    <script src="@Url.Content("~/Areas/CI/Scripts/CIF1012.js?v=" + ASOFTEnvironment.Version)"></script>
    <script src="@Url.Content("~/Areas/CI/Scripts/CIF1013.js?v=" + ASOFTEnvironment.Version)"></script>
}
<div id="contentMaster">
   @Html.ASOFTHeader(new HeaderModel { Title = Html.ASOFTPageTitle(model => model.Title, "CIF1012").ToHtmlString(), Breakcrumb = true })
    <div id="ViewMaster" class="asf-panel-view-master" >
        <div class="asf-panel-master-header">                        
            <div class="asfbtn asfbtn-right">        
                         @if (Model.Toolbar1 != null)
                        {
                            Model.Toolbar1.EventEdit =   "CIF1012Param.btnEdit_Click";
                            Model.Toolbar1.EventDelete = "CIF1012Param.btnDelete_Click";       
                        }
                        @Html.Partial("~/Views/Shared/EditorTemplates/Toolbar.cshtml", Model.Toolbar1 ?? new ASOFTToolBarViewModel())                                    
                 </div>
             </div>
           <div id="viewPartial">
            @Html.Partial("CIF1012M", Model)
        </div>
    </div>
</div>
@Html.ASOFTHidden("CIF1012TeamID", Model.TeamID)
@Html.ASOFTHidden("CIF1012DepartmentID", Model.DepartmentID)
@Html.ASOFTHidden("UrlCIF1010", Url.Action("index"))
@Html.ASOFTHidden("UrlAddEmp", Url.Action("Index", "CIF1013", new { area = "CI" }))
