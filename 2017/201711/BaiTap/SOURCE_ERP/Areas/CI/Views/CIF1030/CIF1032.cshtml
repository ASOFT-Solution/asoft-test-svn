@using ASOFT.ERP.A00.Core.Control;
@using ASOFT.ERP.A00.Core;
@using ASOFT.ERP.A00.Entities.Infrastructure;
@using ASOFT.ERP.A00.Entities.Database;
@using ASOFT.ERP.A00.Entities.Common;
@using ASOFT.ERP.ViewModel;
@using System.Web.UI.WebControls;
@using Kendo.Mvc;
@model ASOFT.ERP.ViewModel.CI.CIF1032ViewModel
@{
    ViewBag.Title = Html.ASOFTPageTitle(m => m.CIF1032Title, "CIF1032").ToHtmlString();
}

@Html.ASOFTHidden("UrlCIF1031", Url.Action("CIF1031"))
@Html.ASOFTHidden("UrlCIF1032M", Url.Action("CIF1032M"))
@Html.ASOFTHidden("UrlCIF1030", Url.Action("Index"))
@Html.ASOFTHidden("UrlUpdate", Url.Action("Update"))
@Html.ASOFTHidden("UrlDelete", Url.Action("Delete"))
@Html.ASOFTHidden("UrlCheckEdit", Url.Action("CheckEdit"))
@Html.ASOFTHidden("anaEdit", Model.AnaID)
@Html.ASOFTHidden("anaTypeEdit", Model.AnaTypeID)

<div id="contentMaster">
    @Html.ASOFTHeader(new HeaderModel {Title = Html.ASOFTPageTitle(m => m.CIF1032Title, "CIF1032").ToHtmlString(), Breakcrumb = true})
    <div id="ViewMaster" class="asf-panel-view-master" >
        <div class="asf-panel-master-header">
            <div class="asfbtn asfbtn-right">
                @Html.Partial("~/Views/Shared/EditorTemplates/Toolbar.cshtml", Model.Toolbar1 ?? new ASOFTToolBarViewModel())
            </div>
        </div>
        <div id="viewPartial">
            @Html.Partial("CIF1032M")
        </div>
    </div>
</div>

@section jsFeature{
    <script src="@Url.Content("~/Areas/CI/Scripts/CIF1030.js?v=" + ASOFTEnvironment.Version)"></script>
    <script src="@Url.Content("~/Areas/CI/Scripts/CIF1031.js?v=" + ASOFTEnvironment.Version)"></script>
}

