@{
    Layout = null;
    if (ViewBag.ActionPrint == 1)
    {
        ViewBag.Action = Url.Action("ExportReport", "CIF1180", new
        {
            id = ViewBag.APK,
            viewer = ViewBag.Viewer,
            actionPrint = "1"
        });
    }
    else
    {
    ViewBag.Action = Url.Action("ExportReport", "CIF1180", new
                                                              {
                                                                  id = ViewBag.APK,
