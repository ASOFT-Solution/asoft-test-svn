

@using ASOFT.ERP.ViewModel.MT
@model ASOFT.ERP.ViewModel.MT.MTF1020ViewModel
@{
    ViewBag.Title = Html.ASOFTPageTitle(model => model.MTF1022Title, "MTF1022").ToHtmlString();
}

@section jsFeature{
    <script src="@Url.Content("~/Areas/MT/Scripts/MTF1020.js")"> </script>
}

@section styleFeature{
    <link href="@Url.Content("~/Areas/MT/Content/MTF1020.css")" rel="stylesheet" type="text/css" />
}

<div id='contentMaster'>
    @Html.ASOFTHeader(new HeaderModel {Title = Html.ASOFTRaw(model => model.MTF1022Title), Breakcrumb = true})
    <div id="mtf1022-viewmaster">
        <div class="asf-panel-view-master-detail mtf1020-view-master">
            <div class="asf-panel-master-header">
                <div class="asfbtn asfbtn-right">
                    @Html.Partial("~/Views/Shared/EditorTemplates/Toolbar.cshtml", Model.Toolbar1 ?? new ASOFTToolBarViewModel())
                </div>
            </div>
            <div id="ViewMaster">
                @Html.Partial("MTF1022M")
            </div>

        </div>
        <div class="asf-panel-view-detail">
            <div class="asfbtn asfbtn-right">
                @Html.Partial("~/Views/Shared/EditorTemplates/Toolbar.cshtml", Model.Toolbar2 ?? new ASOFTToolBarViewModel())
            </div>
            @Html.Partial("GridSchoolTimeDetail", new MTF1021GridViewModel())
        </div>
    </div>

</div>

@(Html.ASOFTPopup()
      .Name("MTF1021Popup")
      .Width(500)
      .Modal(true)
      .Title(Html.ASOFTPageTitle(model => model.MTF1021Title, "MTF1021").ToHtmlString()))

@(Html.ASOFTPopup()
      .Name("MTF1023Popup")
      .Width(420)
      .Modal(true)
      .Title(Html.ASOFTPageTitle(model => model.MTF1023Title, "MTF1023").ToHtmlString()))

@Html.ASOFTHidden("UrlMTF1020", Url.Action("Index"))
@Html.ASOFTHidden("UrlMTF1021", Url.Action("MTF1021"))
@Html.ASOFTHidden("UrlMTF1023", Url.Action("MTF1023"))
@Html.ASOFTHidden("UrlMTF1022M", Url.Action("MTF1022M"))
@Html.ASOFTHidden("UrlInsertDetail", Url.Action("InsertDetail"))
@Html.ASOFTHidden("UrlUpdateDetail", Url.Action("UpdateDetail"))
@Html.ASOFTHidden("UrlDelete", Url.Action("Delete"))
@Html.ASOFTHidden("UrlDeleteDetail", Url.Action("DeleteDetail"))
@Html.ASOFTHidden("UrlUpdate", Url.Action("Update"))
@Html.ASOFTHidden("SchoolTime", Model.SchoolTimeID)