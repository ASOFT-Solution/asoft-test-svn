@using System.Web.UI.WebControls
@using ASOFT.ERP.A00.Core.Control;
@using ASOFT.ERP.A00.Core;
@using ASOFT.ERP.A00.Entities.Common;
@using ASOFT.ERP.ViewModel.MT
@model MTF0070ViewModel
@{
    ViewBag.Title = Html.ASOFTPageTitle(m => m.MTF0070Title, "MTF0070");
}

@section jsFeature{
    <script src="@Url.Content("~/Areas/MT/Scripts/MTF0070.js")"></script>
    <script src="@Url.Content("~/Areas/MT/Scripts/MTF0060.js")"></script>
}

<div id="contentMaster">
      @Html.ASOFTHeader(new HeaderModel() { Title = Html.ASOFTRaw(m => m.MTF0070Title), Breakcrumb = true })
      <div class="full-fieldset">
          <div class="block-left">
              @(Html.ASOFTPanel()
                    .Name("panelbar")
                    .HtmlAttributes(new { @class = "block-panel-master" })
                    .Animation(animation => animation.Enable(true))
                    .Items(panelbar => panelbar.Add().Text(Html.ASOFTRaw(model => model.ReportStudent))
                                           .HtmlAttributes(new { @class = "block-panel-master-header" })
                                           .Expanded(true).Content(Html.Partial("GridStudent").ToHtmlString())))
          </div>
          
          <div class="block-right">
               @(Html.ASOFTPanel()
                    .Name("panelbarrigth")
                    .HtmlAttributes(new { @class = "block-panel-master" })
                    .Animation(animation => animation.Enable(true))
                    .Items(panelbar => panelbar.Add().Text(Html.ASOFTRaw(model => model.ReportExpenses)).HtmlAttributes(new { @class = "block-panel-master-header" })
                                           .Expanded(true).Content(Html.Partial("GridExpenses").ToHtmlString())))
          </div>
      </div>
</div>
@Html.ASOFTHidden("UrlMTF0061", Url.Action("MTF0061", "MTF0060"))
@Html.ASOFTHidden("UrlDoPrintOrExport", Url.Action("DoPrintOrExport", "MTF0070"))
@Html.ASOFTHidden("UrlGetReportFile", Url.Action("GetReportFile", "MTF0070"))
@Html.ASOFTHidden("UrlReportViewer", Url.Action("ReportViewer", "MTF0070"))
<!-- Popup -->
    @(Html.ASOFTPopup()
        .Name("MTF0061Popup")
        .Width(600)
        .Title(Html.ASOFTPageTitle(model => model.MTF0061Title, "MTF0061").ToHtmlString()))