@using ASOFT.ERP.ViewModel.DRM
@model DRF0060ViewModel
@{
    Layout = "~/Views/Shared/_PopupLayout.cshtml";
    ViewBag.Title = Html.ASOFTPageTitle(m => m.DRF0060Title, "DRF0060").ToHtmlString();
    ViewBag.Width = 600;
}

@using (Html.BeginForm(null, null, FormMethod.Post, new {id = "DRF0060"}))
{
 <div class="asf-form-container">
    <div class="form-content">
            <table class="asf-table-view">
                <tr>
                    <td class="drf-td">@Html.ASOFTLabel(model => model.AFeeCoefficient1)</td>
                    <td colspan="2">@Html.ASOFTSpinEdit(model => model.AFeeCoefficient1)</td>
                </tr>
                <tr>
                    <td class="drf-td">@Html.ASOFTLabel(model => model.AFeeCoefficient2)</td>
                    <td colspan="2">@Html.ASOFTSpinEdit(model => model.AFeeCoefficient2)</td>
                </tr>
                <tr>
                    <td class="drf-td">@Html.ASOFTLabel(model => model.AFeeCoefficient3)</td>
                    <td colspan="2">@Html.ASOFTSpinEdit(model => model.AFeeCoefficient3)</td>
                </tr>
            </table>
    </div>
</div>
<div class="asf-form-button">
    <table class="asf-table-view">
        <tr>
            <td align="right">
                <div class="asf-popup-button-container">
                    <div>
                        @(Html.ASOFTButton(ASOFTLanguage.FindWordA00("A00.btnClose"))
                              .Name("BtnClose")
                              .Events(e => e.Click("DRF0060.btnClose_Click")))
                    </div>
                    <div>
                        @(Html.ASOFTButton(ASOFTLanguage.FindWordA00("A00.btnConfig"))
                                .Name("BtnConfig")
                                .Events(e => e.Click("DRF0060.btnConfig_Click")))
                    </div>                          
                </div>
            </td>
        </tr>
    </table>
</div>
}

@section styleFeature{
    <link href="@Url.Content("~/Areas/DRM/Content/Common.css")" rel="stylesheet" type="text/css" />
}

@section jsFeature {
    <script src="@Url.Content("~/Areas/DRM/Scripts/DRF0060/DRF0060.js?v=" + ASOFTEnvironment.Version)"></script>
   @* @Html.RenderScripts(files: new[] { "CRMF2061" })*@
}