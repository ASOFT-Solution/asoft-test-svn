@model ASOFT.ERP.ViewModel.POS.POSF0012ViewModel

@{
    Layout = "~/Views/Shared/_PopupLayout.cshtml";
    ViewBag.Title = @Html.ASOFTPageTitle(@Html.PopupEditTitle(m => m.POSF00121Title), "POSF00121").ToHtmlString();
    ViewBag.Width = 1200;
}

@using (Html.BeginForm(null, null, FormMethod.Post, new { id = "POSF00121" }))
{
    @Html.ASOFTHidden(m=>m.InventoryID)
    <div class="asf-form-container container_12">
        @Html.Partial("GridPromotionInventories")
    </div>
    <div class="container_12 asf-form-button">
        <ul>
            <li>
                @(Html.ASOFTButton(ASOFTLanguage.FindWordA00("A00.btnClose"))
                      .Name("BtnClose")
                      .Events(e => e.Click("popupClose")))

            </li>
        </ul>
    </div>
}

@section jsFeature {
    <script src="@Url.Content("~/Areas/POS/Scripts/ASOFTVIEW.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Areas/POS/Scripts/POSF00121.js")" type="text/javascript"></script>
}